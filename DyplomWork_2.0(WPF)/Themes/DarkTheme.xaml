    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#73a94b"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#437921"/> 

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryGreenColor" Color="#73a94b"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
